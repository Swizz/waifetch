{
	"imports": {
		"lume/": "https://deno.land/x/lume@v3.2.1/",
		"sprinkle/": "https://deno.land/x/sprinkle@v1.1.3/",
		"sprae": "https://esm.sh/sprae@v12.4.5"
	},
	"unstable": [
		"temporal"
	],
	"lock": false,
	"tasks": {
		"lume": "deno run -P=lume lume/cli.ts",
		"build": "deno task lume --config=./_config.ts",
		"serve": "deno task lume --serve --config=./_config.ts",
		"watch": "deno task lume --watch --config=./_config.ts"
	},
	"permissions": {
		"lume": {
			"read": true,
			"write": [
				"./"
			],
			"import": [
				"cdn.jsdelivr.net:443",
				"jsr.io:443",
				"deno.land:443",
				"esm.sh:443",
				"gist.githubusercontent.com:443"
			],
			"net": [
				"0.0.0.0:3000",
				"cdn.jsdelivr.net:443",
				"data.jsdelivr.com:443",
				"jsr.io:443",
				"deno.land:443",
				"esm.sh:443"
			],
			"env": true,
			"run": true,
			"sys": true
		}
	},
	"compilerOptions": {
		"types": [
			"lume/types.ts"
		]
	},
	"fmt": {
		"semiColons": false,
		"useTabs": true
	}
}
